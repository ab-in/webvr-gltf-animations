{
  "name": "webvr-gltf-animations",
  "version": "0.0.1",
  "description": "GLtf workflow for bones & moprh animations from Blender to three.js",
  "repository": {
    "type": "git",
    "url": "git+https://aparacuellos@bitbucket.org/aparacuellos/webvr-gltf-animations.git"
  },
  "main": "index.js",
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "eventemitter3": "2.0.3",
    "ismobilejs": "0.4.1",
    "three": "0.85.2",
    "tween.js": "16.6.0",
    "webvr-polyfill": "0.9.27",
    "webvr-ui": "0.9.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.3.0",
    "budo": "10.0.3",
    "glslify": "6.0.2",
    "uglify-js": "3.0.1"
  },
  "scripts": {
    "start": "budo --open index.js:js/index.js --ssl --dir app --live -- -t babelify -t glslify",
    "build": "browserify index.js -t babelify -t glslify | uglifyjs -cm > app/js/index.js"
  },
  "browserify": {
    "transform": [ "glslify" ]
  },
  "authors": [
    "Arturo Paracuellos <arturo@unboring.net>"
  ],
  "license": "MIT"
}
